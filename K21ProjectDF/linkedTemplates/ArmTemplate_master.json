{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "K21ProjectDF"
		},
		"ls_adls_k21project_repdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_k21project_repdl'"
		},
		"ls_blob_k21projectrepsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_k21projectrepsa'"
		},
		"is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/hbuddana/Azure_Data_Factory_COVID-19_Reporting/refs/heads/main/dataset/ds_ecdc_file_list.json"
		},
		"ls_adls_k21project_repdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://k21projectdlsg2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "K21ProjectDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_k21project_repdl_accountKey": {
						"value": "[parameters('ls_adls_k21project_repdl_accountKey')]"
					},
					"ls_blob_k21projectrepsa_connectionString": {
						"value": "[parameters('ls_blob_k21projectrepsa_connectionString')]"
					},
					"is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('is_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"ls_adls_k21project_repdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_k21project_repdl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}